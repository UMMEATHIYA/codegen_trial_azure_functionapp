{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post", "get"],
      "route": "orchestrators/{functionName}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "durableClient",
      "name": "starter",
      "direction": "in"
    }
  ]
}
